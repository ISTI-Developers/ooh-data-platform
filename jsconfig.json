{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~assets/*": [
        "src/assets/*"
      ],
      "~components/*": [
        "src/components/*"
      ],
      "~config/*": [
        "src/config/*"
      ],
      "~fragments/*": [
        "src/fragments/*"
      ],
      "~pages/*": [
        "src/pages/*"
      ],
      "~/*": [
        "src/*"
      ]
    }
  }
}